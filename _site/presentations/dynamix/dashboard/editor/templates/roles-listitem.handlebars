<li class="ui-field-contain">
    <a href="#editRolePopup" data-rel="popup" 
        class="edit-menu ui-btn ui-corner-all ui-shadow ui-btn-inline ui-icon-edit ui-btn-icon-right ui-btn-b" data-transition="pop" 
        data-position-to="window" id="{{this}}">{{this}}</a>
    <li class="ui-body">
        <fieldset class="ui-grid-b">
            <div class="ui-block-a">
                <center>
                    <div style=" float:left; border-radius: 50%; width: 65px; height: 65px; background: #333;">
                        <img src="img/share-icon.png" class="share-icon"> <br>
                        <span> Share </span>
                     </div>
                 </center>
            </div>
            <div class="ui-block-b">
                <button type="submit" class="ui-btn ui-corner-all ui-icon-action ui-btn-icon-left ui-mini share-qr" id="{{this}}">QR</button>
            </div>
            <div class="ui-block-c">
                <button type="submit" class="ui-btn ui-corner-all ui-icon-action ui-btn-icon-left ui-mini share-nfc" id="{{this}}">NFC</button>
            </div>
        </fieldset>
    </li>
</li>