<div data-deviceid="{{data.deviceId}}">
	<h3>{{data.deviceId}}</h3>
	<br/>
	<div  class="ui-grid-c">
		{{#if data.commands.PLAYBACK_BACKWARD_SEEK}}
			<div class="ui-block-a backwardSeek">
				<center><i class="fa fa-fast-backward fa-2x"></i></center>
			</div>
		{{/if}}
		{{#if data.commands.PLAYBACK_PLAY_PAUSE}}
			<div class="ui-block-b playPause">
				<center><i class="fa fa-play fa-2x"></i><i class="fa fa-pause fa-2x"></i></center>
			</div>

		{{/if}}
		{{#if data.commands.PLAYBACK_FORWARD_SEEK}}
			<div class="ui-block-c forwardSeek">
				<center><i class="fa fa-fast-forward fa-2x"></i></center>
			</div>
		{{/if}}
		{{#if data.commands.DISPLAY_VIDEO}}
			<div class="ui-block-c youtubePlay">
				<center><i class="fa fa-youtube-play fa-2x"></i></center>
			</div>
		{{/if}}
	</div>
</div>
<br/>
